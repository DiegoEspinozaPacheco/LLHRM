# Gmail SMTP configuration
defaults
auth           on
tls            on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile        /tmp/msmtp.log

# Account for Gmail
account gmail
host smtp.gmail.com
port 587
from "you@example.com"
user "you@example.com"
password "APP_PASSWORD_HERE"

account default : gmail
