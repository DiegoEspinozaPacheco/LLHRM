# Gmail SMTP configuration
defaults
auth           on
tls            on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile        /tmp/msmtp.log

# Account for Gmail
account gmail
host smtp.gmail.com
port 587
from your.email@gmail.com
user your.email@gmail.com
password APP_PASSWORD_OR_TOKEN

account default : gmail
